{
    "compilerOptions": {
        "module": "commonjs",
        "noImplicitAny": false,
        "removeComments": false,
        "target": "ES5",
        "sourceMap": true,
        "outDir": "out",
        "noUnusedLocals": true,
        "noImplicitThis": true,
        "noFallthroughCasesInSwitch": true,
        "noImplicitReturns": true
    },
    "exclude": [
        "node_modules",
        "!node_modules/@types",
        "testapp/",
        "testdata/"
    ]
}
