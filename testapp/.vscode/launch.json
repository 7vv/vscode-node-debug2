{
    "version": "0.2.0",
    "configurations": [
        {
            "debugServer": 4712,
            "name": "Launch",
            "type": "node2",
            "request": "launch",
            "program": "${workspaceRoot}/out/app.js",
            "cwd": "${workspaceRoot}",
            "skipFiles": [
                "app2.ts"
            ],
            "smartStep": true,
            "outFiles": ["${workspaceRoot}/out/**/*.js"]
        },
        {
            "name": "Attach",
            "type": "node2",
            "request": "attach",
            "port": 9229,
            "address": "localhost",
            // "sourceMaps": true,
            "outDir": "${workspaceRoot}/out",
            // "localRoot": "${workspaceRoot}",
            // "remoteRoot": "/Users/roblou/Downloads/testapp",
            "trace": "all",
            // "verboseDiagnosticLogging": true
            // "restart": true
        },
        {
            "name": "Attach remote",
            "type": "node2",
            "request": "attach",
            "address": "10.211.55.3",
            "port": 5860,
            "sourceMaps": true,
            "outDir": "${workspaceRoot}/out",
            "diagnosticLogging": true,
            "trace": "all",
            "console": "integratedTerminal",
            "localRoot": "${workspaceRoot}/out",
            // "remoteRoot": "/Users/roblou/code/vscode-node-cdp-debug/testapp/out"
            "remoteRoot": "C:\\Users\\roblou\\code\\vscode-node-cdp-debug\\testapp\\out"
        }
    ]
