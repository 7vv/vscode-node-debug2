{
    "version": "0.2.0",
    "debugServer": "4712",
    "configurations": [
        {
            "name": "Launch",
            "type": "node",
            "request": "launch",
            "program": "${workspaceRoot}/out/app.js",
            // "stopOnEntry": true,
            "cwd": "${workspaceRoot}",
            "runtimeExecutable": "/Users/roblou/Downloads/node-v7.0.0-nightly20160905a290ddfdc9-darwin-x64/bin/node",
            // "runtimeExecutable": "/Users/roblou/.nvm/versions/node/v6.4.0/bin/node",
            "sourceMaps": true,
            "verboseDiagnosticLogging": true,
            "outDir": "${workspaceRoot}/out",
            // "webRoot": "${workspaceRoot}/out",
            // "trace": "all"
            "console": "integratedTerminal"
        },
        {
            "name": "Attach",
            "type": "node",
            "request": "attach",
            "port": 5858,
            "address": "localhost",
            // "sourceMaps": true,
            "outDir": "${workspaceRoot}/out",
            // "localRoot": "${workspaceRoot}",
            // "remoteRoot": "/Users/roblou/Downloads/testapp",
            "trace": "all",
            "verboseDiagnosticLogging": true
            // "restart": true
        },
        {
            "name": "Attach to Process",
            "type": "node",
            "request": "attach",
            "processId": "${command.PickProcess}",
            "port": 5858,
            "sourceMaps": false,
            "outDir": null
        }
    ]
}